projection;
strict ( 2 );
use draft;

define behavior for Z_C_COMPANY //alias <alias_name>
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Prepare;
  use action Resume;

  use action setallpresent;


  use association _employees { create; with draft;}
}

define behavior for Z_C_EMPLOYEES //alias <alias_name>
{
  use update;
  use delete;

  use association _company {with draft;}
}